<launch>
	<rosparam command="load" file="$(find auv_control)/config/altitude.yaml" />
	<rosparam command="load" file="$(find auv_control)/config/depth.yaml" />
	<rosparam command="load" file="$(find auv_control)/config/yaw_tune.yaml" />

	<node name="IMU_update" pkg="auv_control" type="rowpitchyaw.py"></node>
  <node name="balance_PID" pkg="auv_control" type="balancePID.py"></node>
  <node name="depth_PID" pkg="auv_control" type="depthPID.py"></node>
  <node name="forward_PID" pkg="auv_control" type="forwardPID.py"></node>
  <node name="total_force" pkg="auv_control" type="force_sum.py"></node>
  <node name="auv_manager" pkg="auv_control" type="auv_manager.py"></node>
  <node name="depth_update" pkg="auv_control" type="getdepth.py"></node>
  <node name="motor_output" pkg="auv_control" type="inter_linear_py.py"></node>
</launch>
